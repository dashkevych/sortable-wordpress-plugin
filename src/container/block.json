{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "sortable/container",
	"version": "0.1.0",
	"title": "Sortable Container",
	"category": "widgets",
	"icon": "portfolio",
	"description": "Container which holds sortable items.",
	"attributes": {
		"isSorted": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "sortable",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
